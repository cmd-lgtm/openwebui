{
  "name": "nexus-ide",
  "version": "1.0.0",
  "description": "NEXUS IDE â€” AI-Powered IDE with Multi-Agent Orchestration",
  "main": "dist/main/index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:main\" \"npm run dev:renderer\"",
    "dev:main": "electron-vite dev",
    "dev:renderer": "vite dev --config vite.renderer.config.ts",
    "build": "tsc && vite build --config vite.renderer.config.ts && electron-vite build",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "npm run build && electron-builder --mac",
    "build:linux": "npm run build && electron-builder --linux",
    "start": "electron .",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "test": "vitest",
    "test:e2e": "playwright test",
    "postinstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.24.0",
    "@google/generative-ai": "^0.14.0",
    "@monaco-editor/react": "^4.6.0",
    "@radix-ui/react-dialog": "^1.1.0",
    "@radix-ui/react-dropdown-menu": "^2.1.0",
    "@radix-ui/react-tabs": "^1.1.0",
    "@radix-ui/react-tooltip": "^1.1.0",
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/addon-search": "^0.15.0",
    "@xterm/addon-web-links": "^0.11.0",
    "@xterm/addon-webgl": "^0.18.0",
    "@xterm/xterm": "^5.5.0",
    "chokidar": "^3.6.0",
    "cmdk": "^1.0.0",
    "electron-store": "^9.0.0",
    "electron-updater": "^6.2.0",
    "express": "^4.19.0",
    "framer-motion": "^11.2.0",
    "isomorphic-git": "^1.25.0",
    "lucide-react": "^0.400.0",
    "monaco-editor": "^0.50.0",
    "node-pty": "^1.0.0",
    "ollama": "^0.5.0",
    "openai": "^4.52.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "react-markdown": "^9.0.0",
    "react-resizable-panels": "^2.0.0",
    "react-syntax-highlighter": "^15.5.0",
    "remark-gfm": "^4.0.0",
    "socket.io": "^4.7.0",
    "socket.io-client": "^4.7.0",
    "sonner": "^1.5.0",
    "uuid": "^9.0.0",
    "ws": "^8.17.0",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/node": "^20.14.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@types/react-syntax-highlighter": "^15.5.0",
    "@types/uuid": "^9.0.0",
    "@types/ws": "^8.5.0",
    "@vitejs/plugin-react": "^4.3.0",
    "autoprefixer": "^10.4.0",
    "concurrently": "^8.2.0",
    "electron": "^31.0.0",
    "electron-builder": "^24.13.0",
    "electron-vite": "^2.3.0",
    "eslint": "^9.6.0",
    "postcss": "^8.4.0",
    "prettier": "^3.3.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.5.0",
    "vite": "^5.3.0",
    "vitest": "^1.6.0"
  },
  "build": {
    "appId": "com.nexus.ide",
    "productName": "Nexus IDE",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "node_modules/**/*"
    ],
    "win": {
      "target": ["nsis"],
      "icon": "build/icon.ico"
    },
    "mac": {
      "target": ["dmg"],
      "icon": "build/icon.icns"
    },
    "linux": {
      "target": ["AppImage"],
      "icon": "build/icon.png"
    }
  }
}
